    unlockID: 
  - character: {fileID: 11400000, guid: 316923714c86081488bc8a3c70391ad6, type: 2}
    dialogue: "$I know more than you might imagine\u2026\r"
    choices: []
    unlockID: 
  - character: {fileID: 11400000, guid: fb47278d81865c943a1d856966e6d63a, type: 2}
    dialogue: "$Oh, that sounds useful! &Can you tell me about yesterday?\r"
    choices: []
    unlockID: 
  - character: {fileID: 11400000, guid: 316923714c86081488bc8a3c70391ad6, type: 2}
    dialogue: "#I was in my house. I have been in my house, here, by myself all day.\r"
